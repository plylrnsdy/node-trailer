  "keywords": [
    "logger",
    "log"
  ],
  "main": "out/src/index.js",
  "scripts": {
    "build": "ttsc -p .",
    "doc": "jsdoc2md --configure ./jsdoc2md.json --heading-depth=3 --example-lang=js -t README.hbs --files ./src/**/*.ts > ./README.md && typedoc",
