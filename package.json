    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@types/chai": "^4.2.15",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.30",
    "chai": "^4.3.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.3.0",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.20.28",
    "typedoc-plugin-markdown": "^3.5.0",
